generator client {
    provider      = "prisma-client-js"
    binaryTargets = ["native"]
}

datasource db {
    provider = "sqlite"
    url      = "file:./database.sqlite"
}

model Events {
    id        String   @id @unique
    createdAt DateTime @default(now())
    active    Boolean  @default(true)
    title     String
    apiKey    String
}

model Order {
    id            String   @id @unique
    createdAt     DateTime @default(now())
    email         String
    phoneNumber   String
    firstName     String
    lastName      String
    photos        String
    storeNumber   String
    promiseTime   String
    confirmed     Boolean  @default(false)
    vendorOrderId String?
}
